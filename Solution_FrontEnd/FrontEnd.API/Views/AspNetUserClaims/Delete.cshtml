@model FrontEnd.API.Models.AspNetUserClaims

@{
    ViewData["Title"] = "Delete";
}

<div class="container">

    <h1>Eliminar</h1>

    <h3>¿Estás seguro de que quieres eliminar esto?</h3>
    <div>
        <h4>AspNetUserClaims</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ClaimType)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ClaimType)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ClaimValue)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ClaimValue)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.User)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.User.Id)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            <a asp-action="Index">Volver a la lista</a>
        </form>
    </div>
</div>