}
<div class="container">

    <h1>Detalles</h1>

    <div>
        <h4>Correo Tienda</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Correo)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Correo)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Descripcion)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Descripcion)
            </dd>
            <dt class="col-sm-2">
